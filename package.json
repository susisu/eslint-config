{
  "name": "@susisu/eslint-config",
  "version": "0.0.70",
  "description": "My ESLint config",
  "repository": "https://github.com/susisu/eslint-config.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "default": "./index.js"
    },
    "./preset/js": "./preset/js.js",
    "./preset/ts-without-types": "./preset/ts-without-types.js",
    "./preset/ts": "./preset/ts.js"
  },
  "files": [
    "index.js",
    "index.mjs",
    "lib",
    "preset",
    "config",
    "!**/*.test.js"
  ],
  "scripts": {
    "format": "prettier --write '*.js' '*.mjs' lib preset config",
    "format:check": "prettier --check '*.js' '*.mjs' lib preset config",
    "lint": "eslint --fix '*.js' '*.mjs' lib preset config",
    "lint:check": "eslint '*.js' '*.mjs' lib preset config",
    "test": "node --test lib preset config",
    "configtest": "run-s -c 'configtest:*'",
    "configtest:js": "./configtest/js/run.sh",
    "configtest:ts": "./configtest/ts/run.sh",
    "configtest:ts-type-checked": "./configtest/ts-type-checked/run.sh",
    "prepublishOnly": "run-s lint test configtest"
  },
  "dependencies": {
    "@susisu/eslint-plugin-safe-typescript": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1"
  },
  "peerDependencies": {
    "eslint": "^8.50.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.4.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^13.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "~5.3.2"
  }
}
