{
  "name": "@susisu/eslint-config",
  "version": "0.0.72",
  "description": "My ESLint config",
  "repository": "https://github.com/susisu/eslint-config.git",
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.mjs",
        "types": "./lib/index.d.mts"
      },
      "require": {
        "default": "./lib/index.js",
        "types": "./lib/index.d.ts"
      }
    },
    "./preset/js": "./preset/js.js",
    "./preset/ts-without-types": "./preset/ts-without-types.js",
    "./preset/ts": "./preset/ts.js"
  },
  "files": [
    "lib",
    "preset",
    "src",
    "!src/**/*.spec.ts",
    "!src/**/__tests__"
  ],
  "scripts": {
    "format": "prettier --write '*.js' '*.mjs' src preset",
    "format:check": "prettier --check '*.js' '*.mjs' src preset",
    "lint": "eslint --fix '*.js' '*.mjs' src preset",
    "lint:check": "eslint '*.js' '*.mjs' src preset",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --coverage",
    "test:dev": "vitest dev --coverage.enabled --coverage.reporter=text",
    "configtest": "run-s -c 'configtest:*'",
    "configtest:js": "./configtest/js/run.sh",
    "configtest:ts": "./configtest/ts/run.sh",
    "configtest:ts-type-checked": "./configtest/ts-type-checked/run.sh",
    "build": "tsup",
    "prepublishOnly": "run-s format:check lint:check typecheck test configtest build"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.4.1",
    "@susisu/eslint-plugin-safe-typescript": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-define-config": "^2.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.50.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^13.23.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "~5.3.2",
    "vitest": "^1.0.4"
  }
}
